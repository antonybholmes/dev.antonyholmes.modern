<?xml version="1.0" encoding="UTF-8"?>
<classes>
	<class name="component">
		<prop name="padding" value="5" />
		<prop name="border-radius" value="0" />
	</class>

	<class name="rect">
		<prop name="border-radius" value="0" />
	</class>

	<class name="square-borders">
		<inherits class="rect" />
	</class>


	<class name="rounded-rect">
		<prop name="border-radius" value="4" />
	</class>

	<class name="pill">
		<prop name="border-radius" value="999" />
	</class>
	
	<class name="panel">
    <prop name="background-color" value="white" />
  </class>

	<class name="widget">
		<!-- <inherits class="rounded-rect" /> -->
	</class>
	
	<class name="button">
	  <inherits class="widget" />
    <inherits class="rounded-rect" />
  </class>

	<class name="ribbon">
		<prop name="background-color" value="white" />
		<prop name="border-radius" value="0" />
	</class>

	<class name="card">
		<!-- <prop name="border-radius" value="8" /> -->
		<!-- <prop name="border-color" value="colors.gray-32.color-3" /> -->
		<prop name="background-color" value="white" />
	</class>

	<class name="content">
		<prop name="background-color" value="white" />
	</class>

	<class name="content-outline">
		<prop name="border-color" value="colors.gray-32.color-8" />
	</class>

	<class name="spinner">
		<inherits class="content" />
		<inherits class="content-outline" />
		<!-- <inherits class="rounded-rect" /> -->
	</class>

	<class name="content-outline-highlight">
		<inherits class="content-outline" />
		<prop name="border-color" value="colors.gray-32.color-8" />
	</class>

	<class name="content-box">
		<inherits class="content" />
		<inherits class="content-outline" />
	</class>

	<class name="outlined-dialog-button">
		<inherits class="content" />
		<inherits class="content-outline" />
	</class>

	<class name="gray-button">
	  <inherits class="button" />
		<prop name="background-color"
			value="colors.material.dialog.button.outline" />
	</class>

	<class name="primary-dialog-button">
	  <inherits class="button" />
		<prop name="border-color"
			value="colors.theme-32.color-24" />
		<prop name="background-color"
			value="colors.theme-32.color-20" />
	</class>

	<class name="dialog-button">
	  <inherits class="button" />
		<inherits class="content-box" />
	</class>

	<class name="checkbox">
		<prop name="background-color" value="white" />
		<prop name="border-color" value="colors.gray-32.color-6" />
		<prop name="border-radius" value="0" />
	</class>

	<class name="check-switch">
		<prop name="background-color"
			value="colors.gray-32.color-4" />
		<prop name="border-color" value="colors.gray-32.color-8" />
	</class>

	<class name="quick-access-button">
	  <inherits class="button" />
	</class>

	<class name="combobox">
		<prop name="background-color" value="white" />
	</class>

	<class name="slider">
		<prop name="background-color"
			value="colors.theme-32.color-16" />
	</class>

	<keyframe index="to">
		<class name="primary-dialog-button">
			<prop name="border-color"
				value="colors.theme-32.color-28" />
			<prop name="background-color"
				value="colors.theme-32.color-24" />
		</class>

		<class name="checkbox">
			<prop name="background-color"
				value="colors.theme-32.color-14" />
		</class>

		<class name="check-switch">
			<prop name="background-color"
				value="colors.theme-32.color-14" />
			<prop name="border-radius" value="0" />
		</class>

		<class name="content-outline-highlight">
			<prop name="border-color"
				value="colors.gray-32.color-20" />
		</class>

		<class name="ribbon-button">
			<prop name="background-color"
				value="colors.material.dialog.button.outline" />
		</class>

		<class name="outlined-dialog-button">
			<inherits class="ribbon-button" />
		</class>

		<class name="combobox">
			<prop name="background-color" value="white" />
			<prop name="alt-background-color"
				value="colors.material.dialog.button.outline" />
		</class>

		<class name="dialog-button">
			<inherits class="ribbon-button" />
			<prop name="border-color"
				value="colors.material.dialog.button.highlight" />
		</class>

		<class name="theme-button">
			<prop name="background-color"
				value="colors.theme-32.color-14" />
		</class>

		<!-- <class name="widget"> -->
		<!-- <prop name="border-color" -->
		<!-- value="colors.material.gray-highlight" /> -->
		<!-- <prop name="background-color" -->
		<!-- value="colors.material.gray-highlight" /> -->
		<!-- </class> -->
		
		<class name="widget">
      <prop name="background-color"
        value="colors.material.dialog.button.outline" />
    </class>

		<class name="outline">
			<prop name="background-color" value="none" />
		</class>

		<class name="quick-access-button">
			<prop name="background-color"
				value="colors.material.gray-highlight" />
			<prop name="border-radius" value="0" />
		</class>

		<class name="content-outline">
			<prop name="border-color"
				value="colors.material.dialog.button.highlight" />
		</class>

		<class name="slider">
			<prop name="background-color"
        value="colors.theme-32.color-24" />
		</class>

	</keyframe>

</classes>